<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="settingsPunkt">
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_marginBottom">@dimen/textAllign</item>
        <item name="android:layout_marginTop">@dimen/textAllign</item>
        <item name="android:layout_marginEnd">@dimen/allign</item>
        <item name="android:layout_marginStart">@dimen/allign</item>
        <item name="android:textSize">@dimen/settingText</item>
        <item name="android:includeFontPadding">false</item>
        <item name="android:gravity">start|center_vertical</item>
        <item name="android:foreground">?android:attr/selectableItemBackground</item>
        <item name="android:drawableTint">@color/YP_Gray</item>
        <item name="android:drawablePadding">12dp</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
    </style>

    <style name="tgToggle">
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="icTheme">
        <item name="tint">@color/white</item>
    </style>

    <style name="mainBack">
        <item name="android:background">?attr/background</item>
    </style>

    <style name="searchBackground">
        <item name="android:background">@color/white</item>
        <item name="android:textColorHint">@color/searchIcon</item>
        <item name="android:drawableTint">@color/searchIcon</item>
    </style>
</resources>